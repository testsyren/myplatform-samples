<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ProxyEndpoint name="default">
    <Description>App Services JSON Patch proxy endpoint</Description>
    <!-- This proxy's flow/steps are defined in the default.xml target -->
    <HTTPProxyConnection>
        <BasePath>/json-patch-app-services</BasePath>
        <VirtualHost>default</VirtualHost>
        <VirtualHost>secure</VirtualHost>
    </HTTPProxyConnection>
    <RouteRule name="default">
        <TargetEndpoint>default</TargetEndpoint>
    </RouteRule>
</ProxyEndpoint>
