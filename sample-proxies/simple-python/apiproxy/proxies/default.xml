<ProxyEndpoint name="default">
    <PreFlow>
    <Request>
		
	</Request>
	<Response>		
		<Step>
			<Name>pythonscriptpolicy</Name>
		</Step>		
	</Response>
    </PreFlow>
    <HTTPProxyConnection>
        <BasePath>/v1/python</BasePath>
        <VirtualHost>default</VirtualHost>
    </HTTPProxyConnection>     
    <RouteRule name="default">
        <TargetEndpoint>default</TargetEndpoint>
    </RouteRule>
</ProxyEndpoint>
