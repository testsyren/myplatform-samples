<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ExtractVariables async="false" continueOnError="true" enabled="true" name="extract-user-json">
    <DisplayName>extract-user-json</DisplayName>
    <JSONPayload>
        <Variable name="username">
            <JSONPath>$.username</JSONPath>
        </Variable>
      <Variable name="request_pw">
            <JSONPath>$.password</JSONPath>
        </Variable>
      <Variable name="request_pw_new">
            <JSONPath>$.password_new</JSONPath>
        </Variable>
    </JSONPayload>
    <Source clearPayload="false">request</Source>
</ExtractVariables>
