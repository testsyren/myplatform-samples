<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="SetValidTokenResponse">
    <Set>
      <StatusCode>200</StatusCode>
      <ReasonPhrase>OK</ReasonPhrase>
      <Payload contentType="application/json" variablePrefix="%" variableSuffix="#">{"expires_in":"%accesstoken.expires_in#", "user":"%accesstoken.username#"}</Payload>
    </Set>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <AssignTo createNew="true" transport="http" type="response"/>
</AssignMessage>
