<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<BasicAuthentication async="false" continueOnError="false" enabled="true" name="EncodeAppCredentials">
    <Operation>Encode</Operation>
    <IgnoreUnresolvedVariables>false</IgnoreUnresolvedVariables>
  <User ref="appKey"/>
  <Password ref="appSecret"/>
    <AssignTo createNew="true">appCredentials</AssignTo>
</BasicAuthentication>